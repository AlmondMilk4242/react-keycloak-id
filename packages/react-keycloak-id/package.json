{
  "name": "react-keycloak-id",
  "version": "0.0.1",
  "description": "A small package for create middleware using keycloak js",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c rollup.config.js && uglifyjs dist/index.js --output dist/index.js",
